{
  "name": "ia-lab-agentkit",
  "version": "0.1.0",
  "description": "Core schemas, compiler, runtime FSM, tool-gating and eval runner for IA Lab",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./compiler": {
      "import": "./dist/compiler/index.js",
      "types": "./dist/compiler/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "ingest:prototipe": "tsx src/ingest/cli.ts"
  },
  "dependencies": {
    "zod": "^3.22.4",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "commander": "^11.1.0",
    "ora": "^8.0.1",
    "chalk": "^5.3.0",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0"
  }
}
