{
  "name": "ia-lab",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise platform for specifying, versioning, testing and operating AI agents",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "pnpm -r typecheck",
    "db:migrate": "pnpm --filter ia-lab-api db:migrate",
    "db:seed": "pnpm --filter ia-lab-api db:seed",
    "ingest:prototipe": "pnpm --filter ia-lab-agentkit ingest:prototipe",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  }
}
